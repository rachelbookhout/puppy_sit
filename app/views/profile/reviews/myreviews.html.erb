<%@reviews.each do |review| %>
  <div class = "row">
  <div class = "large-12 columns">
  <h1><img src = "<%=review.reviewable.profile_photo%>" width = "80px" height = "80px" ><%=review.reviewable.first_name %>'s review for <%= review.request.dog_name%><img src = "<%=review.request.photo%>" width = "80px" height = "80px"></h1>
  </div>
  </div>
  <div class = "row">
  <div class - "large-12 columns">
  <button class="edit-button"><%=link_to "Edit Review", edit_profile_user_review_path(@user,review)%></button> <button class="edit-button"><%=link_to "Delete Review",profile_user_review_path(@user,review), method: :delete, data: {confirm: "Are you sure"}  %>
  </div>
  </div>
  <%end%>

