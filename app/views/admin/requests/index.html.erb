<% @requests.each do |request| %>
<div class="row">

<div class="large-4 columns">
  <img src = "<%= request.photo %>" width = "200px" height = "200px">
</div>

</br>

<div class="large-8 columns">
  <% if request.responses.empty?%>
    <i class="fi-x"></i>
  <%else%>
    <i class="fi-check"></i>
  <%end%>
  <%= link_to request.dog_name, request_path(request)%> - <%=request.start_time.to_s(:times) %>  -  <%= request.end_time.to_s(:times) %>
</div>
</div>
</br>
<%end%>
